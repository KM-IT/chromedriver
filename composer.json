{
  "name": "lbaey/chromedriver",
  "description": "Utility for installing Chromedriver, whatever platform you are on, for use with Behat",
  "keywords": [
    "Chromedriver",
    "Browser Testing",
    "Selenium",
    "Behat"
  ],
  "version": "1.0",
  "homepage": "https://github.com/lbaey",
  "license": "MIT",
  "autoload": {
    "psr-0": {
      "Lbaey": "src/"
    }
  },
  "authors": [
    {
      "name": "Laurent Baey",
      "email": "laurent.baey@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "type": "composer-plugin",
  "require": {
    "php": ">=7.0",
    "composer-plugin-api": "^1"
  },
  "extra": {
    "class": "Lbaey\\Composer\\ChromeDriverPlugin",
    "chromedriver_version" : "2.27"
  }
}
